{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 Chrome versions",
                        "last 2 Firefox versions",
                        "last 2 Opera versions",
                        "last 2 Safari versions",
                        "ie 11"
                    ]
                }
            }
        ]
    ],
    "plugins": ["transform-vue-jsx", "@babel/plugin-transform-runtime"]
}